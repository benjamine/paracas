
# prepend /usr/local/bin (puts brew over system builtin)
export PATH=/usr/local/bin:$PATH
# set homebrew cask options
export HOMEBREW_CASK_OPTS="--appdir=/Applications"

# autosuggestions
source /usr/local/share/zsh-autosuggestions/zsh-autosuggestions.zsh

# common aliases


# aliases for git/hub
alias clone='hub clone'
alias commit='git c'
alias amend='git amend'
alias s='git s'
alias up='git up'
alias pr='git pr'
alias fork='git fork'
alias feature='git feature'
alias br='git br'
alias hotfix='git hotfix'
alias push='git push origin'
alias pushf='git push -f origin'
